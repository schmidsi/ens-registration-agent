{
  "name": "@ens-agent/mcp-server",
  "version": "0.1.0",
  "exports": "./src/main.ts",
  "tasks": {
    "dev": "deno run --env --allow-net --allow-env --allow-read src/main.ts",
    "http": "deno run --env --allow-net --allow-env --allow-read src/http.ts",
    "test": "deno test --env --allow-net --allow-env --allow-read",
    "register": "deno run --env --allow-net --allow-env --allow-read scripts/register.ts",
    "test-x402": "deno run --env --allow-net --allow-env --allow-read scripts/test-x402.ts",
    "inspector": "BROWSER=none npx @modelcontextprotocol/inspector deno run --env --allow-net --allow-env --allow-read src/main.ts"
  },
  "imports": {
    "hono": "npm:hono@4",
    "@x402/hono": "npm:@x402/hono",
    "@x402/evm/exact/server": "npm:@x402/evm/exact/server",
    "@x402/core/server": "npm:@x402/core/server",
    "@x402/paywall": "npm:@x402/paywall",
    "@x402/paywall/evm": "npm:@x402/paywall/evm",
    "@ensdomains/ensjs": "npm:@ensdomains/ensjs@4",
    "@ensdomains/ensjs/public": "npm:@ensdomains/ensjs@4/public",
    "@ensdomains/ensjs/wallet": "npm:@ensdomains/ensjs@4/wallet",
    "viem": "npm:viem@2",
    "viem/chains": "npm:viem@2/chains",
    "viem/accounts": "npm:viem@2/accounts",
    "@modelcontextprotocol/sdk": "npm:@modelcontextprotocol/sdk@1",
    "@modelcontextprotocol/sdk/server/index.js": "npm:@modelcontextprotocol/sdk@1/server/index.js",
    "@modelcontextprotocol/sdk/server/stdio.js": "npm:@modelcontextprotocol/sdk@1/server/stdio.js",
    "zod": "npm:zod@3",
    "@std/assert": "jsr:@std/assert@1"
  },
  "nodeModulesDir": "auto",
  "compilerOptions": {
    "strict": true
  }
}
